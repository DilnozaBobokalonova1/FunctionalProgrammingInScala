package week4


abstract class Nat {
    def isZero: Boolean
    def predecessor: Nat
    def successor: Nat
    def + (that: Nat): Nat
    def - (that: Nat): Nat
}
object Zero extends Nat{
    def isZero = true
    def predecessor = throw new Error("0.predecessor")
    def successor = new Succ(this
}
class Succ(n: Nat) extends Nat
class Nat {
  
}